{
  "name": "TinyLara/TinyLara",
  "description": "A Simple PHP Framework based on Composer, looks like a Tiny Laravel.",
  "keywords": ["tiny", "simple", "framework", "laravel"],
  "license": "MIT",
  "require": {
    "php": ">=5.4.0",
    "tinylara/tinyrouter": "*",
    "tinylara/tinyview": "*",
    "tinylara/tinyvalidator": "*",
    "illuminate/database": "*",
    "illuminate/pagination": "*",
    "filp/whoops": "*",
    "nette/mail": "*",
    "predis/predis": "*",
    "monolog/monolog": "1.13.1"
  },
  "autoload": {
    "classmap": [
      "app/controllers",
      "app/models",
      "services"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}